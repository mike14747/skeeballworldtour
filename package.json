  "bugs": {
    "url": "https://github.com/mike14747/skeeballworldtour/issues"
  },
  "homepage": "https://github.com/mike14747/skeeballworldtour#readme",
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "mysql2": "^1.6.5",
    "react-router-dom": "^5.0.1"
  }
}
