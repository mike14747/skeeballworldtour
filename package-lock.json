{
    "name": "skeeballworldtour",
    "version": "1.0.0",
    "homepage": "https://www.skeeballworldtour.mikegullo.com",
    "description": "",
    "main": "server.js",
    "scripts": {
        "express": "nodemon server.js",
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:prod": "node server.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
        "client": "cd client && npm run start",
        "install": "if-env NODE_ENV=production || npm run install:dev",
        "install:dev": "cd client && npm install",
        "build": "cd client && npm run build"
    },
    "devDependencies": {
        "babel-eslint": "^10.0.3",
        "chai": "^4.2.0",
        "concurrently": "^5.0.0",
        "eslint": "^6.7.2",
        "eslint-config-standard": "^14.1.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.16.0",
        "eslint-plugin-standard": "^4.0.1",
        "mocha": "^7.0.1",
        "nodemon": "^1.19.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mike14747/skeeballworldtour.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mike14747/skeeballworldtour/issues"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "if-env": "^1.0.4",
        "mysql2": "^1.7.0"
    }
}
